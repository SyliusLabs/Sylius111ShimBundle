{
    "name": "sylius-labs/sylius-1-11-shim-bundle",
    "description": "Adds some functionalities from Sylius 1.11 to Sylius 1.12 and above",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SyliusLabs\\Sylius111ShimBundle\\": "src/"
        },
        "files": [
            "src/aliases.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SyliusLabs\\Sylius111ShimBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sylius project"
        }
    ],
    "require": {
        "php": "^8.0",
        "sylius/sylius": "^1.10",
        "symfony/browser-kit": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "symfony/thanks": false
        }
    }
}
